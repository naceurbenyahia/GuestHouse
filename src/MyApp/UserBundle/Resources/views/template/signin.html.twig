{% trans_default_domain 'FOSUserBundle' %}

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">    
    <title>Guest House | Signin</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="{{ asset('bundles/myapp/img/favicon.ico') }}" type="image/x-icon">
    
    
    <!-- Font awesome -->
    <link href="{{ asset('bundles/myapp/css/font-awesome.css') }}" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="{{ asset('bundles/myapp/css/bootstrap.css') }}" rel="stylesheet">
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href={{ asset('bundles/myapp/css/slick.css') }}"">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/myapp/css/nouislider.css') }}">
    <!-- Theme color -->
    <link id="switcher" href="{{ asset('bundles/myapp/css/theme-color/default-theme.css') }}" rel="stylesheet">

    <!-- Main style sheet -->
    <link href="{{ asset('bundles/myapp/css/style.css') }}" rel="stylesheet">

   
    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Vollkorn' rel='stylesheet' type='text/css'>    
    <link href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
    

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  

  </head>
  <body>
  <section id="aa-signin">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="aa-signin-area">
            <div class="aa-signin-form">
              <div class="aa-signin-form-title">
                <a class="aa-property-home" href="{{ path('my_app_user_homepage') }}">Property GuestHouse</a>
                <h4>Sign in to your account</h4>
              </div>

              <form class="contactform" >
                <div class="aa-single-field">
                  <label for="username">{{ 'security.login.username'|trans }}<span class="required">*</span></label>
                  <input type="text" id="username" name="_username" value="" required="required" />
                </div>

                <div class="aa-single-field">
                  <label for="password">{{ 'security.login.password'|trans }}<span class="required">*</span></label>
                  <input type="password" id="password" name="password" required="required" />
                </div>

                <div class="aa-single-field">
                <label>
                    <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
                    <label for="remember_me">{{ 'security.login.remember_me'|trans }}</label>
                </label>                                                          
                </div>

                <div class="aa-single-submit">
                    <input type="submit" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans }}" />

                  <p>Don't Have A Account Yet? <a href="{{ path('my_app_register') }}">CREATE NOW!</a></p>
                </div>



              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  
  <!-- jQuery library -->
  <!--   <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script> -->
  <script src="{{ asset('bundles/myapp/js/jquery.min.js') }}"></script>
  <!-- Include all compiled plugins (below), or include individual files as needed -->
  <script src="{{ asset('bundles/myapp/js/bootstrap.js') }}"></script>
  <!-- slick slider -->
  <script type="text/javascript" src="{{ asset('bundles/myapp/js/slick.js') }}"></script>
  <!-- Price picker slider -->
  <script type="text/javascript" src="{{ asset('bundles/myapp/js/nouislider.js') }}"></script>
  
  <!-- Custom js -->
  <script src="{{ asset('bundles/myapp/js/custom.js') }}"></script>

  
  </body>
</html>